{
    "compilerOptions": {
      "target": "ES2022",
      "lib": ["DOM", "ES2022"],
      "module": "ESNext",
      "moduleResolution": "Node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "types": ["@cloudflare/workers-types"],
      "baseUrl": ".",
      "paths": {
        "~/*": ["./src/*"]
      },
      "outDir": "dist"
    },
    "include": ["src", "compound-api"],
    "exclude": ["node_modules", "dist"]
  }
  